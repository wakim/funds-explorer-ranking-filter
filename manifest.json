{
  "name": "Filtro para Ranking de FII do Funds Explorer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Essa extens√£o adiciona uma caixa de filtro na tela de rankings de Fii's do site Funds Explorer.",
  "homepage_url": "https://github.com/wakim/funds-explorer-ranking-filter",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://www.fundsexplorer.com.br/ranking*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    },
    {
      "matches": [
        "https://www.fundsexplorer.com.br/ranking*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}